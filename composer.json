{
    "name": "digitalpi/laravel-dto-validator",
    "version": "1.0.1",
    "description": "DTO injector and validator with attributes, inspired by Spring Boot bean validation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pape Samba NDOUR",
            "email": "papesambandour@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "illuminate/http": "^10.0 || ^11.0 || ^12.0",
        "illuminate/validation": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalPi\\DtoValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
